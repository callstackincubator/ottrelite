{
  "name": "@ottrelite/instrumentation-react",
  "version": "0.1.0",
  "description": "Pluggable tracing framework for React Native plugin for Babel - autoinstrumention",
  "main": "lib/index",
  "module": "lib/index",
  "types": "lib/index.d.ts",
  "source": "src/index",
  "author": "Callstack <hello@callstack.com>",
  "files": [
    "src",
    "lib",
    "README.md"
  ],
  "scripts": {
    "postinstall": "pnpm run build || exit 0;",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli android/build node_modules/**/android/build lib",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "lint-ci": "eslint \"**/*.{js,ts,tsx}\" -f @jamesacarr/github-actions",
    "typescript": "tsc",
    "build": "pnpm run typescript",
    "build:watch": "tsc --watch",
    "dev": "concurrently \"pnpm run build:watch\" \"pnpm run test:watch\"",
    "test": "jest --config ../../jest.config.ts",
    "test:watch": "jest --watch --config ../../jest.config.ts",
    "test:update-snapshots": "jest --updateSnapshot --config ../../jest.config.ts",
    "publish:npm": "pnpm publish --access public --no-git-checks",
    "publish:yalc": "yalc publish"
  },
  "keywords": [
    "react-native",
    "babel",
    "telemetry",
    "tracing",
    "trace",
    "observability",
    "performance"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callstackincubator/ottrelite.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callstackincubator/ottrelite/issues"
  },
  "homepage": "https://github.com/callstackincubator/ottrelite#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@babel/types": "^7.28.2",
    "eslint": "^8.57.1",
    "jest": "^30.0.4",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@ottrelite/core": "workspace:^",
    "@babel/core": "^7.28.0",
    "@babel/generator": "^7.28.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/node": "^24.1.0",
    "concurrently": "^9.2.0"
  },
  "peerDependencies": {
    "react": ">=17",
    "@ottrelite/core": "workspace:~"
  }
}
